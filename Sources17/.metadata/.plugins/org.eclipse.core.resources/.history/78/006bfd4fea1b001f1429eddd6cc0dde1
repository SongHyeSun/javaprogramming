package ch05_ex01;

public class ArrayCopy01 {

	public static void main(String[] args) {
		int [ ] a = {1,2,3,4,5,6,7};
		int [ ] b = new int [10];
		// b배열은 10개가 있다. 나머지 배열값은 0으로 처리된다.
		
		for (int i = 0 ; i < a.length ;  i++ ) {
			b[i] = a[i];
		}
		System.out.println("b->" +b);
		//해시코드
	}

}
