package ch06_ex01;

public class Car3Ex {

	public static void main(String[] args) {
		//Car 3 class c1,c2,c3 선언
//		Car3 c1 = new Car3 ("소나타", 10, 14);
//		Car3 c2 = new Car3 ("다마스", 9, 20);
//		Car3 c3 = new Car3 ("모 닝", 12, 13);
		
		//이렇게 생성해주면, Car3의 생성자를 불러와서 자동으로 멤버변수 초기화되고 메모리까지 생성된다.
		Car3 c1 = new Car3("소나타", 10, 14);
		Car3 c2 = new Car3("다마스", 9, 20);
		Car3 c3 = new Car3("모 닝", 12, 13);
		
		c1.print();
		c2.print();
		c3.print();

	}
}
