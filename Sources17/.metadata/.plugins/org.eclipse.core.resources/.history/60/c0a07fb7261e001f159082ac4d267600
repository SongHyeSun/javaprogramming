package ch07_ex03;

public class Computer {
	//	result1	{1,2,3}  -> ComputerEx main에서 받은 값
	//	result2 {1,2,3,4,5}
	int sum1 (int[] values) {
		int sum = 0;
		//						3
		for (int i = 0; i <values.length; i++) {
			sum += values[i];
			//sum = sum + 1 =1
			//sum = 1   + 2 =3
			//sum = 3   + 3 =6
		}
		return sum;
	}
	
	int sum2 (int ... values) {
		int sum = 0;
		
		for(int i = 0; i < values.length; i++) {
			sum += values[i];
		}
		return sum;
	}
	
}
