package ch03_ex02;

//HW01

public class ex0203 {

	public static void main(String[] args) {
		int pencils   = 534;
		int students = 30;
		
		
		
		//학생 한 명이 가지는 연필 수
		int pencilsPerStudent = pencils / students;
		System.out.printf("학생 한 명이 가지는 연필 수 -> d% \n", pencilsPerStudent);
		
		//남은 연필 수
		int pencilsLeft = pencils % students;
		System.out.printf("남은 연필 수 -> d%", pencilsLeft);
		
	}

}
