package ch06_ex01;

public class CarEx {

	public static void main(String[] args) {
		//ctrl space를 누르고 같은 이름이 같다면, package를 잘 보고 선택!!!
		//선언(Instance)       생성
		Car myCar		 = new Car();
		// 객체도 int처럼 선언 시 Car(객체) + 변수 = 이런식으로
		
		Car yourCar;			//객체명 Reference 변수명(변수명만 지정해준 것)
		yourCar = new Car();	//생성
		
		myCar.color = "빨강";
		myCar.speed = 200;
		yourCar.color = "초록";
		yourCar.speed = 150;
		myCar.print();
		yourCar.print();
		

	}

}
