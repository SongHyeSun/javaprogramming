package ch11_ex01;

import java.util.ArrayList;

public class ArrayList03 {

	public static void main(String[] args) {
		ArrayList<String> list = new ArrayList<String>();
		list.add("사과");
		list.add("바나나");
		list.add("귤");
		list.add("오렌지");
		list.add("바나나");
		
		System.out.println("요소의 개수 ->" + list.size());
		System.out.println(">> Iterator 객체 요소 얻기 <<");

	}

}
