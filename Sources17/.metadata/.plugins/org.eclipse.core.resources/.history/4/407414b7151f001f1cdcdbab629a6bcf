package ch08_robot;

public class MainClass {

	public static void main(String[] args) {
		System.out.println("SuperRobot주문. 만들어 주세요.");
		Robot superRobot = new SuperRobot();
		//su + Ctrl space
		superRobot.shape();
		//기본적으로 팔~ 날 수 있습니다
		superRobot.actionBasic();
		//팔, 다리, 몸통이 있죠
		superRobot.actionFly();
		//날 수 있어요
		superRobot.actionFire();
		//대포동 미사일 발사
	}

}
