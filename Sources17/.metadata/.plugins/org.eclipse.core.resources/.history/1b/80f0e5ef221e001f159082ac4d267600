package ch07_ex03;

public class Calculator {
	void powerOn () {
		System.out.println("전원을 켭니다.");
	}
	int plus (int x, int y) {
		int result1 = x+y;
		return result1;
	}
	double divide (int x, int y) {
		double result2 = (double) x/y;
		return result2;
	}
	void powerOff () {
		System.out.println("전원을 끕니다.");
		
	}

}
