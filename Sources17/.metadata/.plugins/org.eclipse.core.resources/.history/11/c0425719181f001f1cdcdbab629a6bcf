package ch08_robot;

import ch08_robot.actions.FireOk;
import ch08_robot.actions.FlyOk;


public class SuperRobot extends Robot {

	public SuperRobot() {
		flyAction = new FlyOk();
		fireAction = new FireOk();  
	}
	
	@Override
	public void shape() {
		System.out.println("기본적으로 팔, 다리, 머리, 몸통 존재. 그리고 날 수 있습니다.");

	}

}
