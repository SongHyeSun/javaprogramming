package ch13_ex01;

import java.io.File;

public class Dir01 {

	public static void main(String[] args) {
//		File file = new File(".");
		File file = new File("src.ch13_ex01");
		
		String [] list = file.list();
		System.out.println("list->"+ list);
		for (String str:list) {
			System.out.println(str);
		}
				
	}

}
