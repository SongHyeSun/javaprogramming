package ch07_ex01;

//Call By Value && Call By Reference  ---> 면접 ***

class Data {
	int kor;
	int eng;
	int math;
	//Call by Reference
	void addRef (Data d1, Data d2) {
		d1.kor = d2.eng;
		System.out.println("Method국어 점수 : " + d1.kor);
	}
}

public class DataEx {

	public static void main(String[] args) {
		//	확보공간의 주소를 가지고 있다
		 Data d1 = new Data();
		 Data d2 = new Data();
		 Data d3 = new Data();
		 d1.kor = 50;
		 d1.math = 70;
		 d2.eng = 80;

	}

}
