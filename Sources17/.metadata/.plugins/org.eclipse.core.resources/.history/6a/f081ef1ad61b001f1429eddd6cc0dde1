package ch05_ex01;

public class ArrTest01 {

	public static void main(String[] args) {
		// [ ]가 하나이면 1차원  [ ] [ ] 2개이면 2차원, 3차원.... 
		int a [ ] [ ] = {{1,2,3,4,5}, 
						 {11,12,13,14,15}, 
						 {21,22,23,24,25}
						 };
		//행열로 표시한다. 1행1열 = 1
		//					  3행 2열 = 22
		
		for (int i=0 ; i <a.length ; i++) {
			for (int j = 0; j<a[i].length; j++) {
				System.out.print(a[i][j] + " \t");
			} //inner for
			System.out.println();
		}
		//out for
	}

}
