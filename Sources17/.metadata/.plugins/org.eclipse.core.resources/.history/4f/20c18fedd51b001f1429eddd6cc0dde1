package ch05_ex01;

public class ArrTest01 {

	public static void main(String[] args) {
		int a [ ] [ ] = {{1,2,3,4,5}, {11,12,13,14,15}, {21,22,23,24,25}};
		for (int i=0 ; i <a.length ; i++) {
			for (int j = 0; j<a[i].length; j++) {
				System.out.println(a[i][j] + " \t");
			} //inner for
			System.out.println();
		}
		//out for
	}

}
