package ch05_ex01;

public class Arr02 {

	public static void main(String[] args) {
		int [ ] a = new int [ ] {23,45,67,789,2};
		
		System.out.println("===== 일반형 For 문 ==========");
		
		for (int i = 0 ; i < a.length ; i++) {
			System.out.printf("a[%d]=%d \n", i , a[i]);
		}
		System.out.println("===== 향상형 For 문 ==========");
		for (int kk : a) {
			System.out.println("kk->"+kk);
		}

	}

}
