package ch08_robot;

import ch08_robot.actions.FlyNo;

public class CheapRobot extends Robot {
	
	//생성자는 C ctrl space 자동완성
	public CheapRobot() {
		//super가 갖고 있는 것
		flyAction = new FlyNo();
	}

	@Override
	//반드시 구현해야하는 abstract method!!
	public void shape() {
		System.out.println("기본적으로 팔, 다리, 머리, 몸통 존재. 그리고 걸을 수 있습니다.");

	}

}
